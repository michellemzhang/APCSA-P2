import static java.lang.System.*;

public class Lab11c
{
   public static void main( String args[] )
   {

	   TriangleThree myTriangleThree = new TriangleThree();
	   myTriangleThree.setTriangle(3, "A");
	   myTriangleThree.getLetter();
	   System.out.print(myTriangleThree.toString());
	   
	   myTriangleThree.setTriangle(7, "X");
	   myTriangleThree.getLetter();
	   System.out.print(myTriangleThree.toString());
	   
	   myTriangleThree.setTriangle(1, "R");
	   myTriangleThree.getLetter();
	   System.out.print(myTriangleThree.toString());
	   
	   myTriangleThree.setTriangle(5, "T");
	   myTriangleThree.getLetter();
	   System.out.print(myTriangleThree.toString());
	   
	   myTriangleThree.setTriangle(4, "W");
	   myTriangleThree.getLetter();
	   System.out.print(myTriangleThree.toString());
	}
}