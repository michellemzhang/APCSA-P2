import static java.lang.System.*;

public class Lab09f
{
	public static void main( String args[] )
	{
		
		
		int k=3;
		String s = "";
		while( k > -1 ){
		  s = k + " " + s;
		  k = k--;   //same as k--;
		}
		System.out.print(s);
	}
}